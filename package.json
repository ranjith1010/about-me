{
  "name": "about-me",
  "version": "1.0.0",
  "description": "Ranjith Thangaraju - Uruvaana kathai",
  "main": "index.html",
  "scripts": {
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "clean": "rimraf dist",
    "copyfonts-fa": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "copyfonts": "copyfiles -f fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run copyfonts && npm run copyfonts-fa && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranjith1010/about-me.git"
  },
  "author": "Ranjith Thangaraju",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ranjith1010/about-me/issues"
  },
  "homepage": "https://github.com/ranjith1010/about-me#readme",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^6.0.0",
    "lite-server": "^2.5.4",
    "onchange": "^7.0.2",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.2",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
